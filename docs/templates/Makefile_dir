################################################################
## Directory Settings
################################################################

TOPDIR := < relative path to top directory >
SUBDIR := < relative path from top directory to this directory >

DIRECTORIES := < 1st subdirectory >
DIRECTORIES += < 2nd subdirectory >

################################################################
## Load Configuration
################################################################

include $(TOPDIR)/env/make/Make.conf
