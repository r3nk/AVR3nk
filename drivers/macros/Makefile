################################################################
## Directory Settings
################################################################

TOPDIR := ../..
SUBDIR := drivers/macros

################################################################
## Sources and Headers
################################################################

SOURCES :=
HEADERS := src/macros_pin.h

################################################################
## Dependencies
################################################################

DEPENDENCIES := 

################################################################
## Supported MCUs
################################################################

MCU := atmega644
MCU += atmega644p
MCU += atmega16

################################################################
## Targets
################################################################

OVERRIDE_DEFAULT_TARGETS = 1

default: headers

clean: 

headers: $(MCU:%=mcu-install-headers-%)

build: 

install: headers

all: headers

################################################################
## Load Configuration
################################################################

include $(TOPDIR)/env/make/Make.conf
